/*向右*/
.freas >a,.seldi a:first-child,.lasle,.arimg a:first-child,.spla li,.qla p a:first-child,.conse a,.fona a:first-child,.main_i li div:first-child{border-width:0px 1px 0 0;}
/*向左*/
.exc,.checo a{ border-width:0px 0 0 1px;}

/*向上、向下*/
.perma,.despl >a,h2.total,a.fimo,.infoli li,footer,.head,.fana,a.resea,.dicon h2,.basic,.dica li,.spacu a,.upda li,.foot,.raatu a,.recom li,.ache li,.choto a,h3.probal,.loswei ul,.prso,.arimg,.spla li,.plres a,.spdi,h3.mayb,.askan li,.stati li,.blood li,.search,.searc a,.docli li,.formst input,.othel,.caninf a,.fipro div,.notice a,.qla p:first-child,.sex a.curs,.conse,.oubor,.vobla input,.infoma li,.datc,.share a.cancle,a.topic p.four,.proto li,.epriv li,.busdo,h2.rec,.fona,.cairet{-webkit-border-image:url(data:image/png;base64,) 2 0 stretch;  border-image:url(data:image/png;base64,) 2 0 stretch; }


.sex a.curs,.oubor,.busdo{border-width:1px 0px 1px 0;}
/*向上*/
a.fimo,footer,a.resea,.basic,.foot,.prso,.arimg,.spdi,.othel,.conse,.share a.cancle,a.topic p.four,.fona,.cairet{border-width:1px 0px 0 0; }

/*向下*/
.perma,.despl >a,h2.total,.infoli li,.head,.fana,.dicon h2,.dica li,.spacu a,.upda li,.raatu a,.recom li,.ache li,.choto a,h3.probal,.loswei ul,.spla li,.plres a,h3.mayb,.askan li,.stati li,.blood li,.search,.searc a,.docli li,.formst input,.caninf a,.fipro div,.notice a,.qla p:first-child,.vobla input,.infoma li,.datc,.proto li,.epriv li,h2.rec{border-width:0 0px 1px 0;}
.asecc div{ border-bottom:0; padding:2rem 0 0 0; background:url(../images/ans_01.png) no-repeat center .95rem; background-size:.8rem .8rem;}
.satis div{ margin:0 auto .9rem auto;}















