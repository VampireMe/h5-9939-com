<script type="text/javascript">
    (function(){
       var article_list = {
           init:function(){
             this.aticleData(0);  
             this.rollEvent();
             this.clickEvent();
           },
           rollEvent:function(){
               $(window).on('scroll',this.rollLoading.bind(this));
           },
           clickEvent:function(){
               $("#ho2").delegate('#mCSB_1 #mCSB_1_container a', "click", this.showDialog.bind(this));
           },
           showDialog:function(abc){
               console.log(abc);
               var id = abc.target.dataset.id;
               this.aticleData(id);  
            },
           aticleData:function(id){
               var data = {
                   'catid': id,
               };
               var that=this;
               japp.ajax.call('news.getArticleList',{
                   'data': data,
                    'beforeSend': function () {},
                    'success': function (ret) {
                        if(ret.code==200){
                            that.navappend(ret.data.article_category,id);
                            that.list1append(ret.data.article_list1);
                            that.list2append(ret.data.article_list2);
                            $(".loading").hide();
                            $(".maincontent").show();
                            that.lunboaa();
                        }else{
                            return false;
                        }
                    },
                    error: function () {
                        console.log('请求失败，请重试');
                    }
               });
           },
           /**
            * 下拉加载更多
            * @returns {undefined}
            */
           rollLoading:function(){
//               alert('aaaa');
//              var pages = $("#noticePage").val();
//              // 当滚动到最底部以上100像素时， 加载新内容
//		if(japp.helper.checkIsScrollToBottom && pages==v){
//                    v=v+1;
//                    this.noticle_data(parseInt(pages)+1);
//                }
            },
            /**
             * 渲染导航
             * @param {type} category
             * @param {type} id
             * @returns {undefined}
             */
            navappend:function(category,id){
                var nav ='<a href="#" data-id="0" class="curre">热点</a>';
                $("#mCSB_1_container a").remove();
                $.each(category, function(k,val2){
                    if(val2.catid==id){
                        $("a").removeClass("curre"); 
                        nav +='<a href="#" data-id="'+val2.catid+'" class="curre">'+val2.catname+'</a>';
                        $(".zjtj").css('display','none');
                    }else{
                        nav +='<a href="#" data-id="'+val2.catid+'">'+val2.catname+'</a>';
                    }
                });
                $("#mCSB_1_container").append(nav);
            },
            list1append:function(list1){
                var html='';
                $(".list1 li").remove();
                $.each(list1, function(i,val){
                  var time = japp.helper.date('Y-m-d',val.inputtime);
                  var title_count = val.title.length;
                  if(title_count>12){
                      val.title = val.title.substring(0,12)+'...';
                  }
                    html +='<li><a href=""><img src="'+val.thumb+'" alt=""><div><h3>'+val.title+'</h3><p><span>'+val.category.substring(0,4)+'<em></em>'+time+'</span><span>收藏0</span></p></div></a></li>';
                });
                $(".list1").append(html);
            },
            /**
             * 渲染咨询列表2
             * @param {type} list2
             * @returns {undefined}
             */
            list2append:function(list2){
                var html1='';
                $(".list2 li").remove();
                $.each(list2, function(ii,val1){
                  var time = japp.helper.date('Y-m-d',val1.inputtime);
                  var title_count = val1.title.length;
                  if(title_count>12){
                      val1.title = val1.title.substring(0,12)+'...';
                  }
                  html1 +='<li><a href=""><img src="'+val1.thumb+'" alt=""><div><h3>'+val1.title+'</h3><p><span>'+val1.category+'<em></em>'+time+'</span><span>收藏0</span></p></div></a></li>';
                });
                $(".list2").append(html1);
            },
            /**
             * 重新加载轮播图
             * @returns {undefined}
             */
            lunboaa:function(){ 
                $(".main_visual").hover(function(){
                        $("#btn_prev,#btn_next").fadeIn()
                },function(){
                        $("#btn_prev,#btn_next").fadeOut()
                });
                $dragBln = false;
                $(".main_image").touchSlider({
                        flexible : true,
                        speed : 200,
                        btn_prev : $("#btn_prev"),
                        btn_next : $("#btn_next"),
                        paging : $(".flicking_con a,.flicking_c a"),
                        counter : function (e){
                                $(".flicking_con a").removeClass("on").eq(e.current-1).addClass("on");
                                $(".flicking_c a").removeClass("on").eq(e.current-1).addClass("on");
                        }
                });
                $(".main_re").touchSlider({
                        flexible : true,
                        speed : 200,
                });

                $(".main_image,.main_re").bind("mousedown", function() {
                        $dragBln = false;
                });
                $(".main_image,.main_re").bind("dragstart", function() {
                        $dragBln = true;
                });
                $(".main_image a,.main_re").click(function(){
                        if($dragBln) {
                                return false;
                        }
                });
                timer = setInterval(function(){
                        $("#btn_next").click();
                }, 5000);
                $(".main_visual").hover(function(){
                        clearInterval(timer);
                },function(){
                        timer = setInterval(function(){
                                $("#btn_next").click();
                        },5000);
                });
                $(".main_image,.main_re").bind("touchstart",function(){
                        clearInterval(timer);
                }).bind("touchend", function(){
                        timer = setInterval(function(){
                                $("#btn_next").click();
                        }, 5000);
                });
            },
       }
       article_list.init();
    }())
</script>
