<script src="/patients/scripts/disease/prevent.js"></script>
<script src="/patients/scripts/disease/neopathy.js"></script>
<script src="/patients/scripts/disease/inspect.js"></script>
<script src="/patients/scripts/disease/treat.js"></script>
<script src="/patients/scripts/disease/food.js"></script>
<script src="/patients/scripts/disease/articles.js"></script>
<script src="/patients/scripts/disease/index.js"></script>

</body>
</html>

